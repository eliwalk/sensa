{
  "manifest_version": 2,

  "name": "Sensa",
  "description": "This extension highlihts sensational words given a rating",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  "content_scripts": [
    {
      "matches": ["*"],
      "css": ["mystyles.css"],
      "js": ["jquery-1.11.3.min.js", "myscript.js"]
    }
  ],
}
